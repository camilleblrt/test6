# ----------------------------------------------------------------------
# Prevent viewing of .htaccess, .htpassword files
# ----------------------------------------------------------------------


<FilesMatch "^\.([Hh][Tt]|[Dd][Ss]_[Ss]|txt)">
        Order allow,deny
        Deny from all
  Satisfy All
</FilesMatch>

<files CHANGELOG>
order allow,deny
deny from all
</files>

# ----------------------------------------------------------------------
# Start rewrite engine
# ----------------------------------------------------------------------

# Turning on the rewrite engine is necessary for the following rules and
# features. FollowSymLinks must be enabled for this to work.

# Some cloud hosting services require RewriteBase to be set: goo.gl/HOcPN
# If using the h5bp in a subdirectory, use `RewriteBase /foo` instead where
# 'foo' is your directory.

# If your web host doesn't allow the FollowSymlinks option, you may need to
# comment it out and use `Options +SymLinksIfOwnerMatch`, but be aware of the
# performance impact: http://goo.gl/Mluzd

<IfModule mod_rewrite.c>
  Options +FollowSymlinks
# Options +SymLinksIfOwnerMatch
  RewriteEngine On
  RewriteBase /
</IfModule>



# ----------------------------------------------------------------------
# Redirect daskeyboard.com to www.daskeyboard.com
# ----------------------------------------------------------------------

<IfModule mod_rewrite.c>
   RewriteCond %{HTTP_HOST} ^daskeyboard.+$ [NC]
   RewriteRule ^ http://www.%{HTTP_HOST}%{REQUEST_URI} [R=301,L]
</IfModule>

# ----------------------------------------------------------------------
# Redirect gaming.daskeyboard.com to www.daskeyboard.com
# ----------------------------------------------------------------------
<IfModule mod_rewrite.c>
    RewriteCond %{HTTP_HOST} ^gaming.daskeyboard.+$ [NC]
    RewriteRule ^(.*)$ http://www.daskeyboard.com$1 [R=301,L]
</IfModule>
# ----------------------------------------------------------------------
# Redirect gaming.daskeyboard.com to staging.daskeyboard.com
# ----------------------------------------------------------------------
<IfModule mod_rewrite.c>
    RewriteCond %{HTTP_HOST} ^gaming.daskeyboard.com$ [NC]
    RewriteRule ^(.*)$ http://staging.daskeyboard.com%{REQUEST_URI} [R=301,NC,L,QSA]
</IfModule>

